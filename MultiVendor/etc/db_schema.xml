			<column name="custom_multivendor_id"/>
		</constraint>
		<column name="sku" nullable="true" xsi:type="varchar" comment="sku" length="255"/>
		<column name="vendor_number" nullable="true" xsi:type="varchar" comment="vendor_number" length="255"/>
		<column name="vendor_note" nullable="true" xsi:type="varchar" comment="vendor_note" length="255"/>
		<column name="created_at" nullable="true" xsi:type="timestamp" comment="created_at"/>
		<column name="updated_at" nullable="true" xsi:type="timestamp" comment="updated_at"/>
	</table>
</schema>

<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<tab id="custom_multivendor" translate="label" sortOrder="999">
			<label>Product Details - Disclaimer</label>
		</tab>
		<section id="general" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
			<label>Edit Product Disclaimer</label>
			<tab>custom_multivendor</tab>
			<resource>Custom_MultiVendor::config_custom_multivendor</resource>
			<group id="product_details" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				<label>Product Details</label>
				<field id="product_disclaimer" type="textarea" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>Product Disclaimer</label>
					<comment/>
				</field>
			</group>
		</section>
	</system>
</config>
